{
  "name": "imdb-front",
  "version": "1.0.0",
  "license": "MIT",
  "main": "index.html",
  "scripts": {
    "start": "parcel index.html --open",
    "build": "buildlite default build"
  },
  "buildlite": {
    "default": {
      "entry": "./index.js",
      "outputPath": "dist",
      "outputName": "[name].[hash].js",
      "style": "style.[hash].css",
      "html": {
        "templatePath": "./public/index.html"
      },
      "copy": [
        {
          "from": "./public",
          "to": "."
        }
      ]
    },
    "dev": {
      "react": "dev",
      "devServer": 8000
    },
    "build": {
      "react": "build",
      "clean": [
        "dist"
      ],
      "publicPath": "https://www.weblite.me:3000/wapp/5d0a90af229bab56f5630b9d"
    }
  },
  "dependencies": {
    "helper": "^0.0.13",
    "imdb-scrapper": "^2.2.0",
    "superagent": "^5.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.2.0",
    "parcel-bundler": "^1.6.1"
  }
}